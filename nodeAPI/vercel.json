{
    "version": 2,
    "builds": [
        {
            "src": "api/v1//*.js",
            "use": "@vercel/node",
            "config": {
                "zeroConfig": true
            }
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "api/v1/index.js"
        }
    ]
}